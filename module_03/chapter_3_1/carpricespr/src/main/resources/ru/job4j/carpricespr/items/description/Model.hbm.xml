<hibernate-mapping>
    <class name="ru.job4j.carpricespr.items.description.Model" table="model">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="name"/>
        <many-to-one name="brand"
                     column="brand_id"
                     class="ru.job4j.carpricespr.items.description.Brand"/>
    </class>
</hibernate-mapping>