<hibernate-mapping>
    <class name="ru.job4j.carpricespr.items.description.Photo" table="photo">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="fileName" column="file_name"/>
        <property name="fileData" column="file_data"/>
        <many-to-one name="car"
                     column="car_id"
                     class="ru.job4j.carpricespr.items.Car"/>
    </class>
</hibernate-mapping>